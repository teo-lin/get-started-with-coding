{
    "pr_winget": {
        "name": "Winget",
        "text": "Winget (Windows Package Manager)",
        "install": "winget install -e Microsoft.VisualStudio.16.PackageDependency",
        "windows_path": "$($env:ProgramFiles)\\WindowsApps\\Microsoft.DesktopAppInstaller_1.*\\AppInstaller\\winget.exe"
    },
    "pr_update": {
        "name": "Update existing Apps",
        "text": "Update existing Apps (...via Winget)",
        "install": "winget upgrade -all"
    },
    "pr_gitcli": {
        "name": "Git",
        "text": "Git (Version control system)",
        "install": "winget install -e Git.Git",
        "windows_path": "$($env:ProgramFiles)\\Git\\bin"
    },
    "pr_docker": {
        "name": "Docker",
        "text": "Docker (Containerization platform)",
        "install": "winget install -e Docker.DockerDesktop",
        "windows_path": "$($env:ProgramFiles)\\Docker\\Docker\\resources\\bin\\docker.exe"
    },
    "pr_vscode": {
        "name": "VSCode",
        "text": "VSCode (Lightweight and powerful code editor)",
        "install": "winget install -e Microsoft.VisualStudioCode",
        "windows_path": "$($env:ProgramFiles)\\Microsoft VS Code\\Code.exe"
    },
    "pr_vscext": {
        "name": "Basic VSCode Extensions",
        "text": "Basic VSCode Extensions (ESLint, Prettier, Docker, Postgres, PowerShell)",
        "install": "code --install-extension dbaeumer.vscode-eslint --install-extension esbenp.prettier-vscode --install-extension ms-azuretools.vscode-docker --install-extension ms-ossdata.vscode-postgresql --install-extension ms-vscode.PowerShell"
    },
    "pr_intelj": {
